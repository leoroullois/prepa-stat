{
  "name": "prepa-stat",
  "version": "1.0.0",
  "description": "Site web aidant les élèves de CPGE dans leur orientation",
  "main": "server.js",
  "scripts": {
    "start": "node build/server.js",
    "start:backend": "nodemon server.ts",
    "start:client": "cd client && npm run start",
    "build:backend": "tsc --project ./",
    "build:client": "cd client && npm run build",
    "build": "npm run build:backend && npm run build:client && cd client/build && cp index.html 200.html",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "scrap": "cd lib && python3 scrap.py",
    "parse": "cd lib && ts-node parse.mts"
  },
  "engines": {
    "node": "14.x"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [
    "SCEI",
    "CPGE",
    "MP",
    "PC",
    "PT",
    "PSI",
    "MPSI",
    "PTSI",
    "PCSI"
  ],
  "homepage": "https://prepa-stat.herokuapp.com",
  "author": "Leyo & LighTender",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/d3": "^7.1.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.21",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "d3": "^3.0.0",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.3.1",
    "mongoose": "^6.1.7",
    "node-fetch": "^2.6.7",
    "passport": "^0.5.2",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.0",
    "@types/express-session": "^1.17.4",
    "@types/is-empty": "^1.2.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/validator": "^13.7.1",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
