{
  "name": "prepa-stat",
  "version": "1.0.0",
  "description": "Site web aidant les élèves de CPGE dans leur orientation",
  "main": "server.js",
  "scripts": {
    "watch": "nodemon server.ts",
    "start": "cd client && npm run start",
    "backend-build": "tsc --project ./",
    "client-build": "cd client && npm run build",
    "build": "npm run backend-build && npm run client-build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "scrap": "cd lib && python3 scrap.py",
    "parse": "cd lib && ts-node parse.mts"
  },
  "engines": {
    "node": "14.x"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [
    "SCEI",
    "CPGE",
    "MP",
    "PC",
    "PT",
    "PSI",
    "MPSI",
    "PTSI",
    "PCSI"
  ],
  "author": "Leyo & LighTender",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "d3": "^3.0.0",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "mongodb": "^4.3.1",
    "mongoose": "^6.1.7",
    "node-fetch": "^2.6.7",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/d3": "^7.1.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/node": "^16.11.21",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
